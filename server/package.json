{
  "name": "huglu-backend",
  "version": "1.0.0",
  "description": "Backend API server for Huglu mobile app",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:prod": "NODE_ENV=production node server-production.js",
    "start:admin": "node admin-server.js",
    "start:app": "node app-api-server.js",
    "start:both": "concurrently \"npm run start:admin\" \"npm run start:app\"",
    "dev": "nodemon server.js",
    "dev:prod": "nodemon server-production.js",
    "dev:admin": "nodemon admin-server.js",
    "dev:app": "nodemon app-api-server.js",
    "dev:both": "concurrently \"npm run dev:admin\" \"npm run dev:app\"",
    "migrate": "node migrate-to-multi-tenant.js",
    "install:prod": "npm install --production",
    "test": "node test-server.js",
    "logs": "tail -f logs/access.log",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop huglu-api",
    "pm2:restart": "pm2 restart huglu-api",
    "pm2:logs": "pm2 logs huglu-api"
  },
  "dependencies": {
    "@google-cloud/speech": "7.2.0",
    "axios": "^1.12.2",
    "basic-ftp": "5.0.5",
    "bcrypt": "5.1.1",
    "compression": "^1.8.1",
    "concurrently": "8.2.2",
    "cors": "2.8.5",
    "crypto": "1.0.1",
    "dotenv": "16.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "7.1.5",
    "express-slow-down": "1.6.0",
    "google-auth-library": "9.14.1",
    "helmet": "7.1.0",
    "hpp": "0.2.3",
    "iyzipay": "2.0.64",
    "jsonwebtoken": "9.0.2",
    "morgan": "^1.10.1",
    "mysql2": "3.14.4",
    "node-cron": "3.0.3",
    "react-native-async-storage": "0.0.1",
    "sequelize": "6.37.7",
    "xml2js": "0.6.2",
    "systeminformation": "5.25.10",
    "redis": "^4.6.13"
  },
  "devDependencies": {
    "@types/crypto-js": "4.2.2",
    "nodemon": "3.0.1"
  },
  "private": true
}
